<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Hotelbookingcode</servlet-name>
        <servlet-class>com.example.Hotelbookingcode</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteOffer</servlet-name>
        <servlet-class>com.example.deleteOffer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>com.example.update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadDBServlet</servlet-name>
        <servlet-class>com.example.FileUploadDBServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendMail</servlet-name>
        <servlet-class>com.example.SendMail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Mailer</servlet-name>
        <servlet-class>com.example.Mailer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Hotelbookingcode</servlet-name>
        <url-pattern>/Hotelbookingcode</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>adminblog</servlet-name>
        <servlet-class>com.example.adminblog</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminblog</servlet-name>
        <url-pattern>/adminblog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteOffer</servlet-name>
        <url-pattern>/deleteOffer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadDBServlet</servlet-name>
        <url-pattern>/FileUploadDBServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendMail</servlet-name>
        <url-pattern>/SendMail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mailer</servlet-name>
        <url-pattern>/Mailer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
